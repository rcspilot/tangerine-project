pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Starting Build'
            }
        }
        stage('Push') {
            steps {
                echo 'Begin Push'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Starting Deploy'
            }
        }
    }
}
